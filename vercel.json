{
  "version": 2,
  "name": "fileuploader",
  "builds": [
    {
      "src": "backend/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Front-End/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "Front-End/$1"
    },
    {
      "src": "/",
      "dest": "Front-End/index.html"
    }
  ]
}
